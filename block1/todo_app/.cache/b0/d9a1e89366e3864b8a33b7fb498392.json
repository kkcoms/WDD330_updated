----------- \n\nColors\n\n-------------------------------- */\n$color-darkest: #3a435e; // Dark Blue\n$color-accent:#7fffd4; //blue green\n$color-light: #FBFBF2; //light yellow\n\n\n","partials/_typography.scss":"/* -------------------------------- \n\nFonts\n\n-------------------------------- */\n\n$primary-font: 'Source Sans Pro', sans-serif;\n$display-font: 'Abril Fatface', cursive;\n\n/* -------------------------------- \n\nxTypography \n\n-------------------------------- */\nbody{\n  font-family: $primary-font;\n}\nh1{\n  font-family: $display-font;\n  color:$color-light;\n  letter-spacing: 2.5px;\n  font-size: 3rem;\n  font-weight: 300;\n  line-height: 1.2;\n  padding:1rem;\n  text-align: center;\n}\n\n\ninput{\n  text-align: center;\n}\np {\n  color:$color-darkest;\n  font-size: 1.1rem;\n  text-transform: capitalize;\n    \n}\n\n//end typography.scss","partials/_basic-style.scss":"/* -------------------------------- \n\nxBasic style\n\n-------------------------------- */\n//basic styles.scss\n*, *::after, *::before {\n\t@include box-sizing(border-box);\n}\n\nhtml * {\n\t-webkit-font-smoothing: antialiased;\n\t-moz-osx-font-smoothing: grayscale;\n}\n\nbody {\n    width: 100%;\n\tmargin: 0 auto;\n\t\n}\nmain {\n\twidth: 100%;\n\n}\n\nheader {\n\twidth: 100%;\n\n}\nli{\n\tpadding:.5rem 1rem;\n}\nfooter{\n\twidth:100%;\n}\n//end basic-styles.scss\n","partials/_prefix-mixins.scss":"@mixin box-sizing($type) {\n  -webkit-box-sizing: $type;\n  -moz-box-sizing: $type;\n  box-sizing: $type;\n}\n\n@mixin clearfix {\n  &::after {\n    clear: both;\n    content: \"\";\n    display: block;\n  }\n}\n\n@mixin transform($options) {\n  -webkit-transform: $options;\n  -moz-transform: $options;\n  -ms-transform: $options;\n  -o-transform: $options;\n  transform: $options;\n}\n\n@mixin transition($options) {\n  -webkit-transition: $options;\n  -moz-transition: $options;\n  transition: $options;\n}\n","partials/_buttons.scss":"\n/* -------------------------------- \n\nbuttons\n\n-------------------------------- */\n.bottomButtons{\n  margin: auto;\n  margin:2rem 0 .5rem 0;\n  padding: 0 1rem;\n  display:flex;\n  justify-content: space-between;\n\n  @include MQ(M){\n    margin: auto;\n    max-width: 80%;\n    justify-content: space-around;\n    \n  }\n\n  @include MQ(L){\n    max-width: 60%;\n  }\n}\n// large button styling\n#task, #submit, #all, #active, #completed{\n  margin: 0 .5rem;\n  padding: 1rem;\n  border-radius: 30px;\n  text-transform: uppercase;\n  letter-spacing: 2.1px;\n  color: $color-darkest;\n  background-color: #fff;\n  border: none;\n  box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);\n  transition: all 0.3s ease 0s;\n  cursor: pointer;\n  outline: none;\n  flex: 1 1 auto;\n\n}\n#submit:hover, #task:hover, #all:hover, #active:hover, #completed:hover {\n  background-color: $color-accent;\n  box-shadow: 0px 15px 20px rgba(46, 229, 157, 0.4);\n  transform: translateY(-7px);\n}\n\n// delete buton\n.fa-trash{\n  color:$color-light;\n  font-size: .9rem;\n  margin:.5rem;\n  padding:.5rem;\n  background-color: $color-darkest;\n  border: none;\n  border-radius: 30px;\n}","partials/_layout.scss":"// breakpoints\n   \n$S:     24em;   \n$M:     46.8em;   \n$L:     50em;  \n$XL:    73em; \n\n// media queries\n\n@mixin MQ($canvas) {\n  @if $canvas == S {\n   @media only screen and (min-width: $S) { @content; } \n  }\n  @else if $canvas == M {\n   @media only screen and (min-width: $M) { @content; } \n  }\n\n  @else if $canvas == L {\n   @media only screen and (min-width: $L) { @content; } \n  }\n\n  @else if $canvas == XL {\n    @media only screen and (min-width: $XL) { @content; } \n   }\n}\n","partials/_main.scss":"/* -------------------------------- \n\nMain Styling\n\n-------------------------------- */\n//header\nheader{\n  background-color:$color-darkest;\n  padding:1rem;\n  margin:auto;   \n}\nmain{\n  background-color: $color-light;\n  padding:2rem 0 2.5rem 0;\n}\n\n// Layout for the block sections\n.todo_list{\n  max-width: 90%;\n  padding:0 1rem;\n  margin: auto;\n  border-radius: 30px;\n\n  @include MQ(M){\n    max-width: 80%;\n  }\n\n  @include MQ(L){\n    max-width: 60%;\n  }\n  \n}\n.todo_item{\n  background-color: white;\n  margin:.6rem 0;\n  display:grid;\n  grid-template-columns: 1fr 2fr 1fr;\n  justify-items:stretch;\n  align-items: center;\n  border-radius: 10px;\n  border-bottom: 5px solid $color-darkest;\n  box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);\n\n  @include MQ(M){\n    margin:1rem 0;\n  }\n\n  @include MQ(L){\n    margin:1.5rem 0;\n  }\n}\n.todo_item_name{\n  justify-self: start;\n  text-align: left;\n}\n\n","partials/_footer.scss":"/* -------------------------------- \n\nfooter\n\n-------------------------------- */ \nfooter{\n    background-color:$color-darkest;\n    padding:1rem;\n    margin:auto;   \n}\n.toDo{\n    margin: auto;\n    display:flex;\n    justify-content: space-between;\n\n    @include MQ(M){\n        max-width: 80%;\n        justify-content: space-around;\n      }\n    \n      @include MQ(L){\n        max-width: 60%;\n      }\n}\n\n\n"},"lineCount":null}},"error":null,"hash":"ae19c5eab1557a02a5f216308c3a0fe3","cacheData":{"env":{}}}