ontent: none;\n}\ntable {\n\tborder-collapse: collapse;\n\tborder-spacing: 0;\n}\n/*end reset.scss*/","partials/_variables.scss":"\n/* -------------------------------- \n\nColors\n\n-------------------------------- */\n$color-accent: #DFEBF2; // Light Blue\n$color-darkest: #26211F; // Dark Brown\n$color-med: #9BB1BF; // medium blue\n$blue-monster:#00ffff;\n$green-monster:#bcd531;\n$orange-monster:#ff6e33;\n$yellow-monster:#fcee21;\n$background: transparent;\n\n\n","partials/_typography.scss":"/* -------------------------------- \n\nFonts\n\n-------------------------------- */\n\n$primary-font: 'Open+Sans', sans-serif;\n$display-font: 'Merriweather', cursive;\n\n\n/* -------------------------------- \n\nxTypography \n\n-------------------------------- */\nbody{\n  font-family: $primary-font;\n}\nh1,\nh2 {\n  font-family: $display-font;\n  color: $color-accent;\n}\n\nh1 {\n  font-size: 3rem;\n  font-weight: 300;\n  line-height: 1.2;\n  padding:1rem;\n\n}\n\nh2 {\n  font-weight: 300;\n  font-size: 2rem;\n  padding:0 1rem;\n  \n}\n\nh3{\n    font-family: $display-font;\n    font-size: 1.5rem;\n    font-weight: 300;\n    color: $color-med;\n    padding:1rem;\n}\n\np {\n  color: $color-med;\n  padding-top:.2rem;\n    \n}\n\na {\n  color: $color-darkest;\n  text-decoration: none;\n  @include transition(0.5s);\n    \n    &:hover{\n        font-size: 1.2em;\n        color: $color-med;\n    }\n}\nfooter{\n  p, a{\n    font-size: .9rem;\n    color:$color-accent;\n  } \n  a{\n    &:hover{\n      font-size: 1.2em;\n      color: $color-darkest;\n  }\n}\n}\n\n\n//end typography.scss","partials/_prefix-mixins.scss":"@mixin box-sizing($type) {\n  -webkit-box-sizing: $type;\n  -moz-box-sizing: $type;\n  box-sizing: $type;\n}\n\n@mixin clearfix {\n  &::after {\n    clear: both;\n    content: \"\";\n    display: block;\n  }\n}\n\n@mixin transform($options) {\n  -webkit-transform: $options;\n  -moz-transform: $options;\n  -ms-transform: $options;\n  -o-transform: $options;\n  transform: $options;\n}\n\n@mixin transition($options) {\n  -webkit-transition: $options;\n  -moz-transition: $options;\n  transition: $options;\n}\n","partials/_basic-style.scss":"/* -------------------------------- \n\nxBasic style\n\n-------------------------------- */\n//basic styles.scss\n*, *::after, *::before {\n\t@include box-sizing(border-box);\n}\n\nhtml * {\n\t-webkit-font-smoothing: antialiased;\n\t-moz-osx-font-smoothing: grayscale;\n}\n\nheader {\n\tmax-width: 100%;\n\n}\nli{\n\tpadding:.5rem 1rem;\n}\nfooter{\n\tmax-width:100%;\n}\nbutton{\n\tbackground-color: brown;\n}\n#makeMonster{\n\twidth:150px;\n\theight:75px;\n\tbackground-color: crimson;\n}\n//end basic-styles.scss \n","partials/_header.scss":"\n/* -------------------------------- \n\nHeader\n\n-------------------------------- */\n//header.scss\nheader {\n  background-color:$color-med;\n \n}\n\n  //end header.scss","partials/_main.scss":"/* -------------------------------- \n\nMain Styling\n\n-------------------------------- */\n\n// Layout for the block sections\n.body_container, .face_container{\n  border-radius: 10px;\n  max-width: 100%;\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  justify-items: center;\n  margin: 1rem;\n  overflow: scroll;\n  background-color: #7fffd4;\n\n  @include MQ(L){\n    margin:1rem auto;\n    max-width: 85%;\n}\n}\n\n","partials/_layout.scss":"// breakpoints\n   \n$S:     24em;   \n$M:     46.8em;   \n$L:     50em;  \n$XL:    73em; \n\n// media queries\n\n@mixin MQ($canvas) {\n  @if $canvas == S {\n   @media only screen and (min-width: $S) { @content; } \n  }\n  @else if $canvas == M {\n   @media only screen and (min-width: $M) { @content; } \n  }\n\n  @else if $canvas == L {\n   @media only screen and (min-width: $L) { @content; } \n  }\n\n  @else if $canvas == XL {\n    @media only screen and (min-width: $XL) { @content; } \n   }\n}\n","partials/_images.scss":"//Body Styling\n.body, .face{\n    border-radius: 10px;\n    margin:.5rem;\n    width:200%;\n    display:flex;\n    align-items: center;\n    justify-content: center;\n    \n}\n// blue body styling\n#blueBody{\n    background-color: $blue-monster;\n}\n\n// green body styling\n#greenBody{\n    background-color: $green-monster;\n}\n\n// orange body styling\n#orangeBody{\n    background-color: $orange-monster;\n}\n\n// yellow body styling\n#yellowBody{\n    background-color: $yellow-monster;\n}\n.face{\n    background-color: $background;\n    border: 1px solid black;\n    padding:2rem;\n\n    img{\n        max-width: 120px;\n\n        @include MQ(M){\n            max-width: 150px;\n\n        }\n        @include MQ(L){\n            max-width: 100%;\n\n        }\n    \n    }\n}\n.body{\n    padding:.5rem;\n\n    img{\n        max-width: 180px;\n        @include MQ(M){\n            max-width: 220px;\n\n        }\n        @include MQ(L){\n            max-width: 100%;\n\n        }\n        \n    \n    }\n}\n// Results Styling\n.results {\n    margin:1rem;\n    padding:1rem;\n    background-color: darkmagenta;\n    max-width: 100%;\n    border-radius: 10px;\n    display: grid;\n    grid-template-columns: .15fr 1fr .15fr;\n\n    @include MQ(L){\n        margin:1rem auto;\n        max-width: 85%;\n        grid-template-columns: .25fr 1fr .25fr;\n    }\n}\n\n.chosenBody {\n    max-width: 100%;\n    grid-area: 1/2;\n    justify-self: center;\n    align-self: center;\n    img{\n        max-width: 100%;\n        justify-self: center;\n        align-self: center;\n    }\n    \n\n}\n.chosenFace {\n    max-width: 50%;\n    margin:0 auto;\n    @include MQ(M){\n        max-width: 70%;\n    }\n    @include MQ(L){\n        max-width: 100%;\n    }\n    grid-area: 1/2;\n    display: grid;\n\n    img{\n        max-width: 80%;\n        justify-self: center;\n        align-self: center;\n        margin-bottom: 1rem;\n\n\n        @include MQ(M){\n            max-width: 75%;\n            margin-bottom: 2rem; \n            \n        }   \n        @include MQ(L){\n            max-width: 100%;\n        } \n    }\n    .variableFace1{\n        max-width: 100%;\n        margin-bottom: 3rem;\n\n        @include MQ(M){\n            max-width: 70%;\n            margin-bottom: 4rem;\n        }\n        @include MQ(L){\n            max-width: 80%;\n        }          \n    }\n    .variableFace3{\n        max-width: 85%;\n\n        @include MQ(M){\n            max-width: 65%;\n            \n        }    \n        @include MQ(L){\n            max-width: 85%;\n        }      \n        \n    }\n    .variableFace4{\n        max-width: 65%;\n        margin-bottom: 1.5rem;\n\n        @include MQ(M){\n            max-width: 70%;\n            margin-bottom: 2rem;\n        }\n        @include MQ(L){\n            max-width: 100%;\n        }        \n    }\n    \n\n}","partials/_footer.scss":"/* -------------------------------- \n\nfooter\n\n-------------------------------- */ \nfooter{\n    background-color: $color-med;\n    padding:1rem;\n    margin:1rem 0;   \n}\n"},"lineCount":null}},"error":null,"hash":"898548f02323ab3fe94b1bf8ae1f512c","cacheData":{"env":{}}}